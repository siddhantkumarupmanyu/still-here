{
  "name": "still-here",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.22",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.3",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^8.0.0"
  },
  "dependencies": {
    "browserify": "^17.0.0"
  },
  "scripts": {
    "backup": "mkdir -p build/src && cp resources-backup/* build/src/",
    "clean:src": "rm -f build/src/*.js",
    "clean:unit": "rm -rf build/test/unit && mkdir -p build/test/unit",
    "clean:end": "rm -rf build/test/end_test && mkdir -p build/test/end_test",
    "clean:src-unit": "yarn clean:src && yarn clean:unit",
    "clean:src-end": "yarn clean:src && yarn clean:end",
    "clean": "yarn clean:src && yarn clean:unit && yarn clean:end",
    "build:src": "tsc --project tsconfig.src.json",
    "build:unit": "tsc --project tsconfig.unit-test.json",
    "build:end": "tsc --project tsconfig.end-test.json",
    "bundle": "yarn browserify ./build/src/main.js --s myBundle -o ./build/src/bundle.js",
    "deploy": "yarn build:src && yarn bundle",
    "run:unit": "yarn jest --config=unit-test.jest.config.js",
    "run:end": "yarn jest --config=end-test.jest.config.js",
    "run:server": "yarn http-server build/src -a localhost -p 4444",
    "test:unit": "yarn clean:src-unit && yarn build:unit && yarn run:unit",
    "test:end": "yarn clean:src-end && yarn deploy && yarn build:end && yarn run:end",
    "unit": "yarn test:unit",
    "end": "yarn test:end",
    "test": "yarn unit && yarn end",
    "clean:gh": "rm -rf ./deploy && mkdir ./deploy",
    "cp:gh": "cp ./build/src/index.html ./build/src/bundle.js ./build/src/style.css ./build/src/mainstyle.css ./build/src/score.css ./deploy",
    "deploy:gh": "yarn deploy && yarn clean:gh && yarn cp:gh"
  }
}